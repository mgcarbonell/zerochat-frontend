re/styles';\nimport logo from '../images/zerochatglitchedlogo.gif'\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    flexGrow: 1,\n    background: 'transparent', \n    boxShadow: 'none'\n  },\n  profileButton: {\n    marginLeft: '1rem'\n  },\n  newEntryButton: {\n    marginRight: '1rem'\n  },\n  img: {\n    maxWidth: '65px',\n  }\n\n}));\n\nconst Navbar = (props) => {\n  const classes = useStyles();\n\n  const [anchorEl, setAnchorEl] = useState(false);\n  \n  const handleMenu = (event) => {\n    setAnchorEl(event.currentTarget);\n  };\n  \n  const handleClose = () => {\n    setAnchorEl(false)\n  };\n\n  useEffect(() => {\n    setAnchorEl(false)\n    return () => {\n      handleClose()\n    }\n  }, [])\n  \n\n  return (\n    <div className={classes.root}>\n      <AppBar \n        position=\"static\" \n        style = {{ backgroundColor: \"black\" }}\n        >\n        <Toolbar>\n          <img className={classes.img} src={logo} alt=\"zerochat logo\" />\n          <Grid\n            component=\"label\"\n            container\n            alignItems=\"center\"\n            spacing={2}\n          >\n          </Grid>\n          <Grid\n            container\n            direction=\"row-reverse\"\n            justify=\"flex-start\"\n            alignItems=\"center\"\n          >\n            { props.currentUser ? \n              <>\n                <IconButton\n                  className={classes.profileButton}\n                  aria-label=\"account of current user\"\n                  aria-controls=\"menu-appbar\"\n                  aria-haspopup=\"true\"\n                  onClick={handleMenu}\n                  color=\"secondary\"\n                  fontSize=\"large\"\n                  >\n                    <AccountCircle \n                      color='secondary' \n                      style={{ fontSize: '300%' }}  \n                    />\n                </IconButton>\n                <Menu\n                  id=\"menu-appbar\"\n                  anchorEl={anchorEl}\n                  anchorOrigin={{\n                    horizontal: 'right'\n                  }}\n                  keepMounted\n                  transformOrigin={{\n                    horizontal: 'right'\n                  }}\n                  open={anchorEl}\n                  onClose={handleClose}\n                >\n                  <MenuItem component={ Link } to={'/profile'} onClick={handleClose}>\n                    Profile\n                  </MenuItem>\n                  <MenuItem component={ Link } to={'/logout'} onClick={handleClose, props.logout}>\n                    Logout\n                  </MenuItem>\n                </Menu>\n              </>\n            :\n              <>\n\n              </>\n            }\n          </Grid>\n        </Toolbar>\n      </AppBar>\n    </div>\n  );\n};\n\nexport default Navbar;","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/JoinChat.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ConnectedUsers.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatMessages.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatMessage.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatTerminal.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatInfoBar.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/CredentialsContainer.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/EditProfile.js",["99"],"import React, { useState } from 'react';\nimport UserModel from '../models/user';\nimport { \n  Button, \n  Grid, \n  Paper, \n  TextField,\n  IconButton \n} from '@material-ui/core';\nimport CloseIcon from '@material-ui/icons/Close'\n\nexport default function EditCreds(props) {\n\n  const [username, setUsername] = useState('');\n  const [email, setEmail] = useState('')\n  const [bio, setBio] = useState('');\n\n  const { setFormToggle } = props\n\n  const handleSubmit = (event) => {\n    event.preventDefault();\n    UserModel.update({ username, email, bio })\n    .then(() => {\n      setFormToggle(false)\n    });\n  };\n\n  const handleClose = () => {\n    setFormToggle(false)\n  }\n\n\n  return (\n    <div style={{ padding: 20 }}>\n      <IconButton\n        onClick={() => handleClose()}\n      >\n        <CloseIcon />\n      </IconButton>\n      <form\n        onSubmit={handleSubmit}\n        noValidate\n        autoComplete=\"off\"      \n      >\n        <Grid\n          container\n          direction=\"column\"\n          justify=\"center\"\n          alignItems=\"center\"\n        >\n          <TextField\n            id='outlined-basic'\n            label='username'\n            type='text'\n            value={username}\n            onInput={event => setUsername(event.target.value)}\n          />\n        </Grid>\n        <Grid\n          container\n          direction=\"column\"\n          justify=\"center\"\n          alignItems=\"center\"\n        >\n          <TextField\n            id='outlined-basic'\n            label='email'\n            type='text'\n            value={email}\n            onInput={event => setEmail(event.target.value)}\n          />\n        </Grid>\n        <Grid\n          container\n          direction=\"column\"\n          justify=\"center\"\n          alignItems=\"center\"\n        >\n          <TextField\n            id='outlined-basic'\n            label='bio'\n            type='text'\n            value={bio}\n            rows={3}\n            onInput={event => setBio(event.target.value)}\n          />\n        </Grid>\n        <Button\n          type=\"submit\"\n        >\n          submit\n        </Button>\n      </form>\n    </div>\n  )\n}\n","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/compon