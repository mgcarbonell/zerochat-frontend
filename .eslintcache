(false);\n\n\n  const handleToggle = () => {\n    setFormToggle(true)\n  };\n\n  const handleDelete = () => {\n    UserModel.delete(props.currentUser)\n    .then(\n      props.history.push('/')\n      )\n      .then(localStorage.clear())\n  };\n\n  return (\n    <div>\n      <h1>Welcome Netrunner</h1>\n      <h3>Credentials</h3>\n      <Paper>\n      { formToggle ?\n      <>\n        <EditCreds\n          username={username}\n          // bio={}\n          formToggle={formToggle}\n          setFormToggle={setFormToggle}\n        />\n      </>  \n      :\n      <>\n        <CredentialsContainer\n          username={username}\n          // bio={}\n        />\n      </>\n      }\n        <IconButton onClick={handleToggle}>\n          <EditIcon />\n        </IconButton>\n        <IconButton aria-label=\"delete\" onClick={() => setConfirmOpen(true)}>\n          <DeleteIcon />\n        </IconButton>\n          <ConfirmDialog\n              title=\"Have you been burned?\"\n              open={confirmOpen}\n              setOpen={setConfirmOpen}\n              onConfirm={handleDelete}\n          >\n            Burn user\n          </ConfirmDialog>\n      </Paper>\n\n      <button>\n        <Link to={ '/join' }>Enter Cyberspace</Link>\n      </button>\n    </div>\n  );\n};\n\nexport default Profile;","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/pages/Login.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/Footer.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/JoinChat.js",["101"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport {\n  Typography\n} from '@material-ui/core'\n\n// use puffs\nconst JoinChat = () => {\n\n  \n  const [node, setNode] = useState('')\n  \n  let username = localStorage.getItem('username')\n\n  return (\n        <div>\n          <div>\n            <Typography variant=\"h2\">Where do you wish to connect on cyberspace?</h2>\n            <div>\n              <input \n                placeholder=\"Node\" \n                type=\"text\" \n                onChange={(event) => setNode(event.target.value)}\n              />\n            </div>\n            <Link \n              onClick={event => !node ? event.preventDefault() : null} \n              to={`/chat?username=${username}&node=${node}`}>\n              <button animate>connect to node</button>\n            </Link>\n          </div>\n        </div>\n  )\n}\n\nexport default JoinChat;","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/Navbar.js",["102"],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/pages/Join.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/pages/Chat.js",["103"],"import React, { useState, useEffect } from 'react'\nimport queryString from 'query-string'\nimport io from \"socket.io-client\";\nimport ChatInfoBar from '../components/ChatInfoBar'\nimport ChatTerminal from '../components/ChatTerminal'\nimport ChatMessages from '../components/ChatMessages'\nimport ConnectedUsers from '../components/ConnectedUsers'\nimport {\n  Box\n} from '@material-ui/core'\n\n\n\nconst ENDPOINT = 'http://localhost:4000'\nlet socket; \n\nconst Chat = ({ location }, props) => {\n  const [username, setUsername] = useState('');\n  const [node, setNode] = useState('');\n  const [users, setUsers] = useState('');\n  const [message, setMessage] = useState('');\n  const [messages, setMessages] = useState([]);\n  \n  \n  useEffect(() => {\n    const { username, node } = queryString.parse(location.search);\n\n    socket = io(ENDPOINT);\n\n    setNode(node);\n    setUsername(username);\n\n    socket.emit('join', { username, node }, (error) => {\n      if(error) {\n        alert(error);\n      }\n    });\n  }, [ENDPOINT, location.search]);\n  \n  useEffect(() => {\n    socket.on('message', message => {\n      setMessages(messages => [...messages, message]);\n    });\n    \n    socket.on(\"nodeData\", ({ users }) => {\n      setUsers(users);\n    });\n}, []);\n\n  const sendMessage = (event) => {\n    event.preventDefault();\n\n    if(message) {\n      socket.emit('sendMessage', message, () => setMessage(''));\n    };\n  };\n\n  console.log(message, messages);\n\n  return (\n      <Box>\n        <Box>\n          <ChatInfoBar node={ node } />\n          <ChatMessages messages={ messages } username={ username }/>\n          <ChatTerminal \n            message={ message }\n            setMessage={ setMessage }\n            sendMessage={ sendMessage }\n          />\n        </Box>\n        <ConnectedUsers users = { users }/>\n      </Box>\n  );\n};\n\n\nexport default Chat;\n","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/EditCreds.js",["104"],"import React, { useState } from 'react';\nimport UserModel from '../models/user';\nimport { \n  Button, \n  Grid, \n  Paper, \n  TextField,\n  IconButton \n} from '@material-ui/core';\nimport CloseIcon from '@material-ui/icons/Close'\n\nexport default function EditCreds(props) {\n\n  const [username, setUsername] = useState('');\n  const [email, setEmail] = useState('')\n  const [bio, setBio] = useState('');\n\n  const { setFormToggle } = props\n\n  const handleSubmit = (event) => {\n    event.preventDefault();\n    UserModel.update({ username, email, bio })\n    .then(() => {\n      setFormToggle(false)\n    });\n  };\n\n  const handleClose = () => {\n    setFormToggle(false)\n  }\n\n\n  return (\n    <div style={{ padding: 20 }}>\n      <IconButton\n        onClick={() => handleClose()}\n      >\n        <CloseIcon />\n      </IconButton>\n      <form\n        onSubmit={handleSubmit}\n        noValidate\n        autoComplete=\"off\"      \n      >\n        <Grid\n          container\n          direction=\"column\"\n          justify=\"center\"\n          alignItems=\"center\"\n        >\n          <TextField\n            id='outlined-basic'\n            label='username'\n            type='text'\n            value={username}\n            onInput={event => setUsername(event.target.value)}\n          />\n        </Grid>\n        <Grid\n          container\n          direction=\"column\"\n          justify=\"center\"\n          alignItems=\"center\"\n        >\n          <TextField\n            id='outlined-basic'\n            label='email'\n            type='text'\n            value={email}\n            onInput={event => setEmail(event.target.value)}\n          />\n        </Grid>\n        <Grid\n          container\n          direction=\"column\"\n          justify=\"center\"\n          alignItems=\"center\"\n        >\n          <TextField\n            id='outlined-basic'\n            label='bio'\n            type='text'\n            value={bio}\n            rows={3}\n            onInput={event => setBio(event.target.value)}\n          />\n        </Grid>\n        <Button\n          type=\"submit\"\n        >\n          submit\n        </Button>\n      </form>\n    </div>\n  )\n}\n","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/CredentialsContainer.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ConfirmDialog.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatInfoBar.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatMessages.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatTerminal.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ConnectedUsers.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/components/ChatMessage.js",[],"/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/theme.js",[],{"ruleId":"105","severity":1,"message":"106","line":13,"column":10,"nodeType":"107","messageId":"108","endLine":13,