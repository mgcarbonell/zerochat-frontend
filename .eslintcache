entDefault();\n\n    if(message) {\n      socket.emit('sendMessage', message, () => setMessage(''));\n    };\n  };\n\n  console.log(message, messages);\n\n  return (\n      <div className={classes.root}>\n        <Grid className={classes.grid}>\n            <ChatInfoBar \n              node={ node } \n            />\n            <ChatMessages \n              messages={ messages } \n              username={ username }\n            />\n            <ChatTerminal \n              message={ message }\n              setMessage={ setMessage }\n              sendMessage={ sendMessage }\n            />\n        </Grid>\n        <ConnectedUsers users={ users }/>\n      </div>\n  );\n};\n\n\nexport default Chat;\n","/home/mcarbonell/sei/final-project/zerochat-pern/zerochat-frontend/src/pages/Join.js",[],{"ruleId":"101","severity":1,"message":"102","line":14,"column":10,"nodeType":"103","messageId":"104","endLine":14,"endColumn":13},{"ruleId":"101","severity":1,"message":"105","line":14,"column":15,"nodeType":"103","messageId":"104","endLine":14,"endColumn":21},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":107,"column":83,"nodeType":"112","messageId":"113","endLine":107,"endColumn":84},{"ruleId":"101","severity":1,"message":"114","line":6,"column":3,"nodeType":"103","messageId":"104","endLine":6,"endColumn":8},{"ruleId":"115","severity":1,"message":"116","line":57,"column":6,"nodeType":"117","endLine":57,"endColumn":33,"suggestions":"118"},"no-unused-vars","'bio' is assigned a value but never used.","Identifier","unusedVar","'setBio' is assigned a value but never used.","no-native-reassign",["119"],"no-negated-in-lhs",["120"],"no-sequences","Unexpected use of comma operator.","SequenceExpression","unexpectedCommaExpression","'Paper' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has an unnecessary dependency: 'ENDPOINT'. Either exclude it or remove the dependency array.